{
	"version": "0.1.2",
	"private": true,
	"scripts": {
		"dev": "yarn run development",
		"dev-js": "yarn run development-js",
		"dev-css": "yarn run development-css",
		"development": "concurrently \"yarn run dev-css\" \"yarn run dev-js\"",
		"development-js": "mix --mix-config webpack.mix.js",
		"development-css": "mix --mix-config webpack.css.mix.js",
		"watch": "concurrently \"yarn run watch-css\" \"yarn run watch-js\"",
		"watch-js": "mix watch --mix-config webpack.mix.js",
		"watch-css": "mix watch --mix-config webpack.css.mix.js",
		"watch-poll": "mix watch -- --watch-options-poll=1000",
		"hot": "mix watch --hot",
		"prod": "yarn run production",
		"prod-js": "yarn run production-js",
		"prod-css": "yarn run production-css",
		"production": "concurrently \"yarn run production-css\" \"yarn run production-js\"",
		"production-js": "mix --mix-config webpack.mix.js --production",
		"production-css": "mix --mix-config webpack.css.mix.js --production",
		"routes": "php artisan ziggy:generate \"resources/js/components/routes/ZiggyExport.js\"",
		"lang": "php artisan vue-i18n:generate",
		"serve": "php artisan serve --host=0.0.0.0 --port=8000",
		"test": "node_modules/.bin/jest --config=jest.config.js",
		"dev-ssr": "concurrently \"npm_config_section=server yarn --section=server run dev\" \"npm_config_section=client yarn --section=client run dev\"  --kill-others-on-fail",
		"dev-ssr-client": "npm_config_section=client yarn --section=client run dev",
		"dev-ssr-server": "npm_config_section=server yarn --section=server run dev",
		"watch-ssr": "concurrently \"npm_config_section=server yarn run watch\" \"npm_config_section=client yarn run watch\"",
		"prod-ssr": "concurrently \"npm_config_section=server yarn --section=server run prod\" \"npm_config_section=client yarn --section=client run prod\"  --kill-others-on-fail"
	},
	"devDependencies": {
		"@babel/core": "^7.20.7",
		"@babel/preset-env": "^7.20.2",
		"@intlify/vue-i18n-loader": "^1.1.0",
		"@vue/test-utils": "^1.3.3",
		"babel-jest": "^26.6.3",
		"concurrently": "^5.3",
		"cross-env": "^7.0",
		"gsap": "^3.11.4",
		"i": "^0.3.7",
		"intersection-observer": "^0.12",
		"jest": "^26.6.3",
		"jest-html-reporter": "^3.7.0",
		"js-cookie": "^3.0",
		"laravel-mix": "^6.0.15",
		"laravel-mix-merge-manifest": "^2.1.0",
		"node-fetch": "^2.6.7",
		"plyr": "^3.7.3",
		"resolve-url-loader": "^5.0.0",
		"sass": "^1.57.1",
		"sass-loader": "^10.4.1",
		"simplebar": "^5.3.9",
		"svg-spritemap-webpack-plugin": "^4.5.0",
		"svg4everybody": "^2.1.9",
		"tiny-slider": "^2.9.4",
		"umbrellajs": "^3.3.1",
		"vanilla-lazyload": "^13.0.1",
		"vue": "^2.7.14",
		"vue-i18n": "^8.28.2",
		"vue-inline-svg": "^1.3.1",
		"vue-jest": "^4.0.1",
		"vue-loader": "^15.10.1",
		"vue-meta": "^2.4.0",
		"vue-router": "^3.6.5",
		"vue-server-renderer": "^2.7.14",
		"vue-template-compiler": "^2.7.14",
		"vuex": "^3.6.2",
		"webpack-bundle-analyzer": "^4.7.0",
		"webpack-livereload-plugin": "^3.0.2"
	},
	"test": "test",
	"dependencies": {
		"@googlemaps/markerclusterer": "^2.0.14",
		"@yeger/vue2-masonry-wall": "^2.3.1",
		"bootstrap": "v5.2",
		"fuse.js": "^6.6.2",
		"google-maps": "^4.3.3",
		"klaro": "^0.7.18",
		"mobile-device-detect": "^0.4.3",
		"oidc-client": "^1.11.5",
		"ts-overlapping-marker-spiderfier": "^1.0.3",
		"vue-dropdowns": "^1.1.2",
		"vue-matomo": "^3.14.0-0",
		"vue-suggestion": "^2.1.0",
		"vuex-oidc": "^3.11.1",
		"vuex-router-sync": "^5.0.0"
	},
	"browserslist": [
		"last 2 versions",
		"not IE 11"
	]
}
